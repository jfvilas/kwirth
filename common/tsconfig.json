{
  "include": [
    "./src"
  ],
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "./src",
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "declaration": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
